              <h4>Projeto: {{project.nome}} </h4>\n                <h4>Autor</h4>\n                <p>{{project.autor}}</p>\n                <h4>Tecnologias</h4>\n                <div v-for=\"tecnologia in project.tecnologias\" :key=\"tecnologia\">\n                    <p>{{tecnologia}}</p>\n                </div>\n                <h4>Link</h4>\n                <p>{{project.link}}</p>\n                <button v-on:click=\"removeProject()\" type=\"submit\" value=\"\" class=\"btn btn-primary\">Excluir</button>\n                <hr/>\n            </div>\n        \n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n    data() {\n        return {\n            projects: [],\n            autor: '',\n            nome: '',\n            tecnologias: '',\n            link: '',\n            }\n        },\n    methods: {\n        addProject() {\n            this.projects.push(\n                {\n                    autor: this.autor,\n                    nome: this.nome,\n                    tecnologias: this.tecnologias.split(','),\n                    link: this.link\n                }\n            );\n            this.autor = ''\n            this.nome = ''\n            this.tecnologias = ''\n            this.link = ''\n        },\n\n        removeProject(index) {\n            if (window.confirm('Deseja mesmo excluir?'))\n                this.projects.splice(index, 1);\n        }\n    }\n}\n</script>"]}]}